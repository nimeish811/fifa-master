    <script type="text/javascript">

    function submit_team(){
      
      var url_string = window.location.href;
              var url = new URL(url_string);
var neckname = url.searchParams.get("neckname");


        var team = [];
       
          $.each($("input[name='team[]']:checked"), function(){            
              team.push($(this).val());
          });

          var len = team.length;
          if(len != 1){
            $("#msg_error").text('Please Select Only 1 Team');
          }else{

  var options = {
    // address: "yMzUo7mZQ8ZHAUhtoNQHZMJe8rTJZ3x1JR",
      address: "TPLXtp614r9Df8hdj34aaGGor4ef1che11",
      amount: 10000000000, // 1 TRVC = 100,000,000
      message: neckname,
      identifier: "446f7261"
    };

    BizApp.payToWallet(options, function(error, result) {

      if (error) {

        $("#result").text("Payment failed: " + error);
        return false;

      }else{

              var url_string = window.location.href;
              var url = new URL(url_string);
              var wallet = url.searchParams.get("wallet");
              var neckname = url.searchParams.get("neckname");
              var necknamearr =  neckname.split("?");
             var neckname = necknamearr[0];
              
              var team_json = JSON.stringify(team); 
              var url_data = "https://ypblocknews.com/fifa/admin_master/user_team_group_submit/";

                 $.ajax({
                    type: 'POST',
                    url: url_data,
                    data: {team:team_json,wallet:wallet,neck:neckname},
                    success : function(data) {
                        if(data==1){
                          window.location.href="group_point.bcml?wallet="+wallet+"&neckname="+neckname;
                        }
                    }
                });

      }
    });

          }
          
    }

    function onload(){

        var url_string = window.location.href;
        var url = new URL(url_string);
        var group = url.searchParams.get("group");
        var url_data = "https://ypblocknews.com/fifa/admin_master/get_team_group_api/"+group;

        $.ajax({
          type: 'GET',
          url: url_data,
          //contentType: 'application/json; charset=utf-8',
          //data: json,
          dataType: 'json',
          success : function(data) {
              var team = data.team_list;

               var html_review = "";
               var ctn = 1;
                var team_name="";
              $.each( team, function( key, value ) {
             
                        var name  = value.team_name;
                        var flag = value.flag;

                        //if(ctn == 1 || ctn ==  5 || ctn == 9 || ctn == 13 || ctn == 17 || ctn == 21 || ctn == 25 || ctn == 29 || ctn ==  23){

                          // team_name = "<span>Group Name : "+value.group_name+"</span>";
                      //  }

                             html_review += '<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3 col-flex" data-filter="CONMEBOL"><style>.fi-team-card .st_1{ fill: url(#SVGID_1); }</style>'+
                     //team_name+
                    '<div class="fi-team-card__bg">'+
                      
                      '<svg x="0px" y="0px" viewBox="0 0 290 320" style="enable-background:new 0 0 290 320;" xml:space="preserve">'+
              '<linearGradient id="SVGID_1" gradientUnits="userSpaceOnUse" x1="0" y1="160" x2="290" y2="160">'+
                      '<stop offset="0" style="stop-color:#BDB289"></stop>'+
                      '<stop offset="0.5174" style="stop-color:#F1EBE3"></stop>'+
                      '<stop offset="1" style="stop-color:#BDB289"></stop>'+
              '</linearGradient>'+
                      
                      '<path class="st_1" d="M290,245.7c0,25.9-14.7,39.2-38.3,39.2c-31.5,0-77.9,5.4-106.5,34.8c-0.2,0.1-0.2,0.2-0.2,0.2 s0-0.1-0.1-0.2C116.2,290.4,69.8,285,38.3,285C14.7,285,0,271.6,0,245.7L0,0h290L290,245.7z"></path>'+                     


                               '</svg>'+
                    '</div>'+
                    '<div class="fi-team-card__img">'+
                    '</div>'+
                    '<div class="fi-team-card__flag">'+
                        '<img src="'+flag+'" alt="ARG" title="ARG" class="fi-ARG fi-flag--5">'+
                    '</div>'+
                    '<div class="fi-team-card__info">'+
                      '<div class="fi-team-card__name">'+
                        name+
                      '</div>'+
                                      '<input type="checkbox" name="team[]" id="squaredTwo" value="'+value.id+'">'+
                                      '<label for="squaredTwo"></label>'+
                    '</div>'+

                '</div>';

                        $("#team_list").html(html_review);
                        ctn++;
              });


              }
            });
      }
onload();



