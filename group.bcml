<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    <script type="text/javascript">

function submit_wallet(){

       var url_data = "https://ypblocknews.com/fifa/admin_master/is_team__group_created/";
       var wallet = $("#wallet").val();
       var neck_name =$("#neck_name").val();
  if(wallet!=""){
        $.ajax({
          type: 'POST',
          url: url_data,
          //contentType: 'application/json; charset=utf-8',
          //data: json,
         data: {wallet:wallet,neck:neck_name},
          success : function(data) {
            if(data==1){
              window.location.href="group_point.bcml?wallet="+wallet+"&neckname="+neck_name;
            }else{
              window.location.href="group_list.bcml?wallet="+wallet+"&neckname="+neck_name;
            }
          }
        });
        
        }else{
        
        $("#msg_error").text("Enter Wallet ID");
        
        }
  }

        function get_money(){

        var url_data = "https://ypblocknews.com/fifa/admin_master/total_group_money/";

            $.ajax({
              url: url_data,
              success : function(data) {

                $("#total_money").text("TOTAL POOL NOW: "+ data+ " TRVC");
              }
            });

        }
              
//get_money();
</script>
