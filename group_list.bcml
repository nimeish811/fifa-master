<script type="text/javascript">
   var url_string = window.location.href;
              var url = new URL(url_string);
var neckname = url.searchParams.get("neckname");
var wallet = url.searchParams.get("wallet");
var ahtml = "";
var grp = "";
    for (var i = 1; i <= 8; i++) {
      if(i==1){
        grp = "A";
      }else if(i==2){
        grp = "B";
      }else if(i==3){
        grp = "C";
      }else if(i==4){
        grp = "D";
      }else if(i==5){
        grp = "E";
      }else if(i==6){
        grp = "F";
      }else if(i==7){
        grp = "G";
      }else if(i==8){
        grp = "H";
      }
      ahtml += "<a href='group_team.bcml?group="+grp+"&wallet="+wallet+"&neckname="+neckname+"' class='btn-social-counter btn-social-counter--instagram' > Group "+grp+"</a>"; 

    };
    $("#ahtml").html(ahtml);



</script>
